{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext}: {Message:lj} | {Properties:j}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Jwt": {
    "Issuer": "paytrack",
    "Audience": "paytrack",
    "ExpirationMinutes": 60,
    "Secret": ""
  }
}
